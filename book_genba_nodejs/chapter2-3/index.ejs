<%
var dt = new Date();
var y = dt.getFullYear();
var m = dt.getMonth();
var d = dt.getDay();
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title><%=title %></title>
<link type="text/css" href="./style.css" rel="stylesheet">
</head>
<body>
<h1><%=title %></h1>
<p><%=message %></p>
<p>Today : <%=y + " Year " + (m+1) + " Month " + d + " Day" %> .</p>
</body>
</html>

